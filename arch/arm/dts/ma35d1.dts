/dts-v1/;
#include "ma35d1.dtsi"

/ {
	model = "Nuvoton MA35D1 EVB board";
	compatible = "nuvoton,ma35d1";

	aliases {
		spi0 = &qspi0;
	};

	memory {
		device_type = "memory";
		reg = <0x00000000 0x80000000 0 0x0e000000>;
	};

};

&uart0 {
	status = "okay";
};

&qspi0 {
        status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;
	spi-nand@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-nand";
		spi-max-frequency = <30000000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <4>;
		reg = <0>;

		partition@0 {
			label = "uboot";
			reg = <0 0x200000>;
		};
		partition@1 {
			label = "uboot-env";
			reg = <0x200000 0xC00000>;
		};
		partition@2 {
			label = "linux-device-tree";
			reg = <0x280000 0x400000>;
		};
		partition@3 {
			label = "linux";
			reg = <0x300000 0xB00000>;
		};

		partition@4 {
			label = "rootfs";
			reg = <0xE00000 0x6E00000>;
		};
	};
};



